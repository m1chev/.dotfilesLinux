#!/usr/bin/env bash

# Get week number
alias week='date +%V'

# Show hidden files
alias ll='ls -la'

# Create parent diretories on demand
alias mkdir='mkdir -pv'

# Ping stop after 5 count packets
alias ping='ping -c 5'

# Useful commands
# alias bc='bc -l' # calculator

# Start jMeter
alias jmeter='cd ~/jMeter/bin/ && ./jmeter'
